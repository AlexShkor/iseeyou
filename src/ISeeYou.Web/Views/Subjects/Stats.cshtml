@model System.Collections.Generic.IEnumerable<ISeeYou.Web.Controllers.SourceItem>

<table class="table">
    <tr>
        <th></th>
        <th>
            Источник
        </th>
        <th>
            Количество лайков
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td><img src="@item.Avatar"/></td>
            <td><a href="http://vk.com/id@(item.UserId)">@item.Name</a></td>
            <td>@item.Count</td>
            <td><a href="@Url.Action("SourceEvents",new{subjectId = ViewBag.subjectId, id = item.UserId})">Фотки</a></td>
        </tr>
    }
</table>